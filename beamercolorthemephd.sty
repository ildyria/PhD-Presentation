% Ume√• University theme colors}

\definecolor{PhDBlue}{RGB}{18,47,87}
\definecolor{PhDGreen}{RGB}{87,18,144}
\definecolor{PhDGold}{RGB}{215,177,124}
\definecolor{PhDPink}{RGB}{234,186,185}

\definecolor{PhDDarkGreen}{RGB}{18,87,23}
\definecolor{PhDDarkGold}{RGB}{215,177,124}
\definecolor{PhDDarkPink}{RGB}{87,18,82}
\definecolor{PhDDarkGrey}{RGB}{125,125,125}

% Normal text
\setbeamercolor{normal text}{fg=black}

% First page
\setbeamercolor*{title}{fg=white,bg=gray}
\setbeamercolor*{author}{fg=white,bg=gray}
\setbeamercolor*{date}{fg=white,bg=gray}

% Generic frames header and footer
\setbeamercolor{titlelike}{fg=black}
\setbeamercolor*{frametitle}{fg=white}
\setbeamercolor*{framesubtitle}{fg=white}
\setbeamercolor*{title in head/foot}{fg=black}
\setbeamercolor*{author in head/foot}{fg=black}

%Blocks
\setbeamercolor{block title}{bg=PhDBlue,fg=white}
\setbeamercolor{block title alerted}{bg=PhDPink,fg=white}
\setbeamercolor{block title example}{bg=PhDGreen,fg=white}
\setbeamercolor{block body}{bg=PhDBlue!15,fg=black}
\setbeamercolor{block body alerted}{bg=PhDPink!20,fg=black}
\setbeamercolor{block body example}{bg=PhDGreen!20,fg=black}

% Lists
\setbeamercolor*{item}{fg=PhDBlue}

% Quotation and quote
\setbeamercolor*{quotation}{fg=black,bg=PhDBlue!10}

% Verse
\setbeamercolor*{verse}{fg=black,bg=PhDBlue!10}

% Abstract
\setbeamercolor*{abstract}{fg=black,bg=PhDBlue!10}

% Table of contents 
\setbeamercolor*{structure}{fg=black}

% Bibliography
\setbeamercolor*{bibliography entry title}{fg=black}
\setbeamercolor*{bibliography entry author}{fg=black}
\setbeamercolor*{bibliography entry location}{fg=black!50}
\setbeamercolor*{bibliography entry note}{fg=black!50}


\mode
<all>